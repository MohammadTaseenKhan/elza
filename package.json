{
  "name": "elza-browser",
  "description": "Elza incognito web browser",
  "version": "0.1.7",
  "author": {
    "name": "ExpertEvidence.org",
    "email": "team@expertevidence.org",
    "url": "https://www.expertevidence.org/"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "productName": "Elza Browser",
  "build": {
    "appId": "com.elza.browser",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:8080/"
      }
    ],
    "directories": {
      "buildResources": "build"
    },
    "linux": {
      "category": "Network",
      "synopsis": "Elza incognito web browser",
      "target": [
        "appImage"
      ],
      "desktop": {
        "Name": "Elza",
        "Comment": "Elza incognito web browser"
      }
    }
  },
  "dependencies": {
    "@imjs/electron-differential-updater": "^5.0.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "blob-stream": "^0.1.3",
    "bootstrap": "^4.5.2",
    "cross-env": "^7.0.2",
    "custom-electron-titlebar": "^3.2.4",
    "dns": "^0.2.2",
    "electron-context-menu": "^2.3.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.4",
    "electron-updater": "^4.3.4",
    "font-awesome": "^4.7.0",
    "nedb": "^1.8.0",
    "pdfkit": "^0.11.0",
    "public-ip": "^4.0.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "sha256": "^0.2.0",
    "url": "^0.11.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "electron": "^9.2.1",
    "electron-builder": "^22.8.0",
    "electron-packager": "^15.1.0"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "react-build": "react-scripts build",
    "electron": "electron .",
    "build-win": "npm run react-build && electron-builder --win -c.extraMetadata.main=build/electron.js",
    "build-linux": "npm run react-build && electron-builder --linux -c.extraMetadata.main=build/electron.js",
    "package-win": "npm run react-build && electron-packager . ElzaBrowser --overwrite --asar --platform=win32 --arch=x64 --prune=true --out=packages",
    "package-linux": "npm run react-build && electron-packager . ElzaBrowser --overwrite --asar --platform=linux --arch=x64 --prune=true --out=packages"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}