{
  "name": "elza-browser",
  "version": "0.1.7",
  "homepage": "./",
  "main": "public/electron.js",
  "build": {
    "appId": "com.elza.browser",
    "files": ["build/**/*", "node_modules/**/*"],
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:8080/"
      }
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "linux": {
    "category": "Utility",
    "synopsis": "Incognito  Browser",
    "target": ["appImage"]
  },
  "dependencies": {
    "@imjs/electron-differential-updater": "^5.0.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "blob-stream": "^0.1.3",
    "bootstrap": "^4.5.2",
    "cross-env": "^7.0.2",
    "custom-electron-titlebar": "^3.2.4",
    "dns": "^0.2.2",
    "electron-context-menu": "^2.3.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.4",
    "electron-updater": "^4.3.4",
    "font-awesome": "^4.7.0",
    "nedb": "^1.8.0",
    "pdfkit": "^0.11.0",
    "public-ip": "^4.0.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "sha256": "^0.2.0",
    "url": "^0.11.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "electron": "^9.2.1",
    "electron-builder": "^22.8.0",
    "electron-packager": "^15.1.0"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "electron": "electron .",
    "eject": "react-scripts eject",
    "build-win": "electron-builder --win -c.extraMetadata.main=build/electron.js",
    "build-lin": "electron-builder --linux -c.extraMetadata.main=build/electron.js",
    "deploy": "electron-builder build --lin --publish never -c.extraMetadata.main=build/electron.js",
    "package-win": "npm run build && electron-packager . ElzaBrowser --overwrite --asar --platform=win32 --arch=x64 --icon='./src/assets/logo.ico' --prune=true --out=release-builds",
    "package-linux": "npm run build && electron-packager . ElzaBrowser --overwrite --asar --platform=linux --arch=x64 --icon='./src/assets/logo.png' --prune=true --out=release-builds"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
