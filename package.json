{
  "name": "elza-browser",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "bootstrap": "^4.5.0",
    "custom-electron-titlebar": "^3.2.3",
    "electron-is-dev": "^1.2.0",
    "font-awesome": "^4.7.0",
    "peerjs": "^1.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "uuid": "^8.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "package-win": "npm run build && electron-packager . Elza-hackp --overwrite --asar --platform=win32 --arch=x64 --icon='./public/logo.ico' --prune=true --out=release-builds --electron-version=9.1.1",
    "package-linux": "npm run build && electron-packager . Elza-hackp --overwrite --asar --platform=linux --arch=x64 --icon='./public/logo.png' --prune=true --out=release-builds --electron-version=9.1.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
